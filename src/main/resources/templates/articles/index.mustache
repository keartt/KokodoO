{{>layouts/header}} <!-- 게시판 목록 /articles -->

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
    </tr>
    </thead>
    <tbody>

    <tr>
        <td>Kim</td>
        <td><a href="/articles/example/kim" > 일본 여행기 </a></td>
    </tr> <!-- 사이트 내 여행 계획 제공값 -->
    <tr>
        <td>Park</td>
        <td><a href="/articles/example/park" > 일본 여행기2 </a></td>
    </tr> <!-- 사이트 내 여행 계획 제공값 -->
    <tr>
        <td>Jung</td>
        <td><a href="/articles/example/jung" > 일본 여행기3 </a></td>
    </tr> <!-- 사이트 내 여행 계획 제공값 -->
    <tr>
        <td>Hwan</td>
        <td><a href="/articles/example/hwan" > 일본 여행기4 </a></td>
    </tr> <!-- 사이트 내 여행 계획 제공값 -->

    {{#articleList}}    <!-- 해당 변수가 데이터의 묶음일 경우 내부를 반복 -->
    <tr>
        <td>익명 {{id}}</td>
        <td> <a href="/articles/{{id}}"> {{title}} </a> </td> <!-- 목록에서 제목 표시시 상세페이지로 이동하게 a 태그로 링크 -->
    </tr>
    {{/articleList}}

    </tbody>

</table>
<a href="/articles/new" class="btn btn-primary"> 내 계획 공유하기 </a> <!-- articles 페이지에서 articles/new 페이지로 링크 -->
{{>layouts/footer}}

